{
    "name": "acat/jwt",
    "description": "Validates, decodes, and authorizes tokens (e.g. JWT)",
    "type": "library",
    "license": "MIT",
    "version" : "0.0.1",
    "config": {
        "optimize-autoloader": true,
        "process-timeout": 0
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "ACAT\\JWT\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    },
    "require": {
        "php": ">=8.4",
        "lcobucci/jwt": "^5.5.0",
        "psr/log": "^3.0.2",
        "phpseclib/phpseclib": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.2.7"
    }
}
